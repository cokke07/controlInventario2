pipeline{
    agent any
    stages {
        stage('Build'){
            steps{
                echo 'Construyendo la aplicacion'
            }
        }
        stage('Test'){
            steps{
                echo 'Ejecutar los test'
            }
        }
        stage('Deploy'){
            steps{
                echo 'Desplegando el area de desarrollo'
            }
        }
    }
}